set -euo pipefail

# Global Variables
LOG_FILE="log.log}"

# Logging helper
log_message() {
  local level="$1"; shift
  local message="$*"
  local timestamp
  timestamp=$(date '+%Y-%m-%d %H:%M:%S')
  local entry="$timestamp [$level] $message"
  printf '%s\n' "$entry" >> "$LOG_FILE"
  printf '%s\n' "$entry"
}

